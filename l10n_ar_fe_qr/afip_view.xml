<?xml version="1.0" encoding="UTF-8"?>
<odoo>


	<record model="ir.ui.view" id="qr_fe_account_move_view_form">
		<field name="name">qr.fe.account.move.form</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="l10n_ar_afipws_fe.view_move_form"></field>
		<field name="arch" type="xml">
			<xpath expr="//notebook/page[@name='afip']/group" position="after">
				<group>
					<field name="json_qr" readonly="1"></field>
					<field name="texto_modificado_qr" readonly="1"></field>
				</group>
			</xpath>
		</field>
	</record>

</odoo>
